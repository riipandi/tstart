{
    "name": "fumadocs-start",
    "private": true,
    "sideEffects": false,
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "build": "pnpm --silent run typecheck && vite build",
        "start": "PORT=3000 node --env-file-if-exists=.env.local .output/server/index.mjs",
        "postinstall": "fumadocs-mdx",
        "cleanup": "pnpm dlx del-cli .{output,wrangler} pnpm-lock.yaml node_modules",
        "update-deps": "npm-check-updates --configFileName .ncurc.json",
        "typecheck": "fumadocs-mdx && tsc -b --noEmit",
        "check": "biome check . --write",
        "format": "biome format . --write",
        "lint": "biome lint . --write"
    },
    "dependencies": {
        "@tanstack/react-router-devtools": "1.136.18",
        "@tanstack/react-router": "1.136.18",
        "@tanstack/react-start": "1.136.18",
        "fumadocs-core": "16.3.1",
        "fumadocs-mdx": "14.1.1",
        "fumadocs-ui": "16.3.1",
        "lucide-react": "^0.562.0",
        "react-dom": "^19.2.3",
        "react": "^19.2.3",
        "tailwind-merge": "^3.4.0"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.3.10",
        "@tailwindcss/vite": "^4.1.18",
        "@types/mdx": "^2.0.13",
        "@types/node": "^25.0.3",
        "@types/react-dom": "^19.2.3",
        "@types/react": "^19.2.7",
        "@vitejs/plugin-react": "^5.1.2",
        "nitro": "3.0.1-alpha.1",
        "npm-check-updates": "^19.2.0",
        "tailwindcss": "^4.1.18",
        "typescript": "^5.9.3",
        "vite-tsconfig-paths": "^6.0.3",
        "vite": "^7.3.0"
    }
}
